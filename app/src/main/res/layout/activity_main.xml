<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".mainActivity">


    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/map"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_marginTop="50dp"
        android:layout_width="match_parent"
        android:layout_height="450dp"

        tools:layout="@layout/activity_navigation" />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:layout_gravity="center"
        android:paddingHorizontal="20dp"
        android:gravity="center">



            <Button
                android:id="@+id/callButton"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="15dp"
                android:background="@drawable/call_emergency_no1"
                android:text="Call Number 1"
                android:layout_marginTop="515dp"
                android:textAllCaps="false"
                android:textColor="#FFFFFF"
                android:textSize="18dp" />

            <Button
                android:id="@+id/callButton2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="15dp"
                android:background="@drawable/call_emergency_no2"
                android:text="Call Number 2"
                android:textAllCaps="false"
                android:textColor="#FFFFFF"
                android:textSize="18dp" />



        <Button
            android:id="@+id/sendButton"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/send_location_btn"
            android:layout_marginBottom="13dp"
            android:text="Send Location"
            android:textSize="18dp"
            android:textColor="#FFFFFF"
            android:textAllCaps="false"/>

        <Button
            android:id="@+id/callFire"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/call_fire_station_btn"
            android:paddingHorizontal="15dp"
            android:layout_marginBottom="10dp"
            android:text="Call Fire Station"
            android:textSize="18dp"
            android:textColor="#FFFFFF"
            android:textAllCaps="false"/>
    </LinearLayout>


</FrameLayout>
